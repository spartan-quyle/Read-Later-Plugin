{
  "name": "read-later-extension",
  "displayName": "Read Later",
  "version": "0.0.1",
  "description": "Archive windows and restore them later.",
  "author": "Antigravity",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:safari": "plasmo build --target=safari-mv2",
    "package": "plasmo package"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "contextMenus",
      "tabs",
      "tabGroups",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}